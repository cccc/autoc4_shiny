{
    "server": "autoc4",
    "port": 9000,
    "debug": {
        "message": false,
        "connect": true,
        "disconnect": true,
        "configLoaded": true
    },
    "modules": [{
            "module": "shutdown",
            "options": {
                "forceShutdownButtonSelector": "#force-shutdown",
                "shutdownButtonSelector": "#shutdown"
            }
        },
        {
            "module": "dmx",
            "options": {
                "roomDataAttribute": "data-dmx-room",
                "channelsDataAttribute": "data-dmx-channels",
                "lightDataAttribute": "data-dmx-light",
                "roleDataAttribute": "data-dmx-role",
                "valueDataAttribute": "data-dmx-value"
            },
            "subscribe": ["dmx/+/+"]
        },
        {
            "module": "heartbeat",
            "subscribe": ["heartbeat/#"]
        }
    ],
    "module_locations": {
        "dmx": "js/autoc4_dmx.js",
        "shutdown": "js/autoc4_shutdown.js",
        "heartbeat": "js/autoc4_heartbeat.js"
    }
}